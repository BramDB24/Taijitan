@model IEnumerable<G07_Taijitan.Models.Domain.OefeningType>
@{
    ViewData["Title"] = "Oefening types";
}

<link href="~/css/website.css" rel="stylesheet" type="text/css">

<body>
    <div class="container">
        <h2>Thema's</h2>

        <div>
            @foreach(var item in Model) {
                <div class="col-sm-6 col-md-3">
                    <a class="keuze text-center" asp-controller="Graad" asp-action="Oefening" asp-route-graadid="@ViewData["Graad"]" asp-route-typeid="@((int)item)">
                        @item
                    </a>
                </div>
            }
        </div>
    </div>
</body>